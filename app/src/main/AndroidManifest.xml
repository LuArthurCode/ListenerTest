<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.listenertest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListenerTest">
        <activity
            android:name=".FourActivity"
            android:label="@string/title_activity_four"
            android:theme="@style/Theme.ListenerTest.NoActionBar"></activity>
        <activity
            android:name=".ThreeActivity"
            android:label="@string/title_activity_three"
            android:theme="@style/Theme.ListenerTest.NoActionBar" />
        <activity
            android:name=".SecondeActivity"
            android:label="@string/title_activity_seconde"
            android:theme="@style/Theme.ListenerTest.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>